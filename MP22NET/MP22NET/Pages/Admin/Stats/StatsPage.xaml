<UserControl x:Class="MP22NET.Pages.Admin.TabPage1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="350">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="Tab" SelectedSource="/Pages/Admin/Stats/MoneySection.xaml">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Sections" Source="/Pages/Admin/Stats/MoneySection.xaml" />
                <mui:Link DisplayName="Caisses" Source="/Pages/Admin/Stats/MoneyCheckout.xaml"/>
                <mui:Link DisplayName="Paniers" Source="/Pages/Admin/Stats/CartsByCheckout.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
